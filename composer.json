{
  "name": "piboutique/simple-cms",
  "description": "Simple CMS that gets it done",
  "type": "project",
  "require": {
    "php": "^7.2|^8.0.0",
    "illuminate/support": ">=5.0",
    "artesaos/seotools": ">=0.16.0",
    "doctrine/dbal": "^2.10|^3.0",
    "spatie/laravel-sitemap": "^5.7|^6.0"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Andres Abello",
      "email": "andresabello@piboutique.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Piboutique\\SimpleCMS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "src/tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Piboutique\\SimpleCMS\\SimpleCMSServiceProvider"
      ],
      "aliases": {}
    }
  }
}
